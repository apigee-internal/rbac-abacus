{
  "compileOnSave": false,
  "compilerOptions": {
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "outDir": "./m-coverage/build"
  },
  "exclude": [
    "node_modules",
    "build",
    "m-coverage",
    "npm",
    "spec"
  ]
